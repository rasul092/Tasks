#include <iostream>
using namespace std;

int main() {
    int say, eded, cem = 0;

    cout << "Neçə ədəd daxil etmək istəyirsiniz? ";
    cin >> say;

    for (int i = 1; i <= say; i++) {
        cout << i << "-ci ədədi daxil edin: ";
        cin >> eded;
        cem += eded;
    }

    cout << "Daxil edilən ədədlərin cəmi: " << cem << endl;

    return 0;
}
