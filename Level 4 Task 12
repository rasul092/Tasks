#include <iostream>
using namespace std;

int main() {
    int n, sum = 0;
    cout << "Ededi daxil edin: ";
    cin >> n;

    for (int i = 1; i < n; i++) {
        if (n % i == 0)
            sum += i;
    }

    if (sum == n)
        cout << "Mukemmel ededdir.";
    else
        cout << "Mukemmel deyil.";

    return 0;
}
